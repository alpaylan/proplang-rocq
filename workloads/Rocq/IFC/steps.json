{
  "setup_steps": [
    {
      "Command": {
        "command": "coqc",
        "args": [
          "--print-version"
        ]
      }
    }
  ],
  "build_steps": [
    {
      "Command": {
        "command": "${workload_path}/build"
      }
    },
    {
      "Command": {
        "command": "${workload_path}/build_generator",
        "args": [
          "!{generator}"
        ]
      }
    },
    {
      "Command": {
        "command": "${workload_path}/build_fuzzer",
        "args": [
          "!{fuzzer}"
        ]
      }
    }
  ],
  "test_steps": [
    {
      "Match": {
        "value": "strategy",
        "options": {
          "fuzzer": {
            "Command": {
              "command": "./main_exec",
              "args": [
                "./${strategy}_exec ${property}"
              ],
              "run_at": "${workload_path}"
            }
          },
          "generator": {
            "Command": {
              "command": "${workload_path}/${strategy}_test_runner.native",
              "args": [
                "${property}"
              ]
            }
          }
        }
      }
    }
  ],
  "tags": {
    "generator": [
      "BespokeGenerator",
      "TypeBasedGenerator"
    ],
    "fuzzer": [
      "TypeBasedFuzzer",
      "VariationalFuzzer"
    ]
  }
}