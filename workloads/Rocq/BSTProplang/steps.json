{
  "setup_steps": [
    {
      "Command": {
        "command": "coqc",
        "args": [
          "--print-version"
        ]
      }
    }
  ],
  "build_steps": [
    {
      "Command": {
        "command": "${workload_path}/build"
      }
    },
    {
      "Command": {
        "command": "${workload_path}/build_generator",
        "args": [
          "!{generator}"
        ]
      }
    }
  ],
  "test_steps": [
    {
      "Command": {
        "command": "${workload_path}/${strategy}_sampler.native",
        "args": [
          "${property}",
          "${tests}"
        ]
      }
    }
  ],
  "tags": {
    "generator": [
      "BespokeGenerator",
      "CoverageGenerator"
    ]
  }
}