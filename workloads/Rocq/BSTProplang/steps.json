{
  "setup_steps": [
    {
      "Command": {
        "command": "make",
        "run_at": "${workload_path}/../Lib"
      }
    },
    {
      "Command": {
        "command": "make",
        "args": [
          "install"
        ],
        "run_at": "${workload_path}/../Lib"
      }
    }
  ],
  "build_steps": [
    {
      "Command": {
        "command": "./build",
        "run_at": "${workload_path}"
      }
    },
    {
      "Command": {
        "command": "./build_generator",
        "run_at": "${workload_path}",
        "args": [
          "!{generator}"
        ]
      }
    }
  ],
  "test_steps": [
    {
      "Command": {
        "command": "./${strategy}_test_runner.native",
        "run_at": "${workload_path}",
        "args": [
          "${property}"
        ]
      }
    }
  ],
  "tags": {
    "generator": [
      "BespokeGenerator",
      "CoverageGenerator"
    ]
  }
}